{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/src/include/kernel/**",
                "${workspaceFolder}/src/include/std/**"
            ],
            "defines": [],
            "compilerPath": "/path-to-gcc-x86_64-elf/bin/x86_64-elf-gcc",
            "compilerArgs": [
                "-ffreestanding",
                "-fno-exceptions",
                "-fno-rtti",
                "-mcmodel=kernel",
                "-m64",
                "-march=x86-64",
                "-mno-red-zone",
                "-msse4.2",
                "-nostdlib",
                "-lgcc"
            ],
            "cStandard": "c17",
            "intelliSenseMode": "linux-gcc-x64",
            "cppStandard": "c++23"
        }
    ],
    "version": 4
}
